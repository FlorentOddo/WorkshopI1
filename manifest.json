{
  "manifest_version": 3,
  "name": "WatWeP",
  "description": "This extension is existing",
  "version": "1.0",
  "permissions": [
    "storage", 
    "history",
    "tabs",
    "activeTab",
    "notifications"
  ],
  "action": {
    "default_popup": "file.html",
    "default_icon":{
      "16": "/img/logo.png",
      "48": "/img/logo.png",
      "128": "/img/logo.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}